#include<iostream>

using namespace std;

int vowelCounter(char arr[], int size)
{
	int index = 0;
	for (int i = 0; i < size; i++)
	{
		if (arr[i] == 'a' || arr[i] == 'e' || arr[i] == 'i' || arr[i] == 'o' || arr[i] == 'u' || arr[i] == 'A' || arr[i] == 'E' || arr[i] == 'I' || arr[i] == 'O' || arr[i] == 'U')
			index++;
	}
	return index;
}
