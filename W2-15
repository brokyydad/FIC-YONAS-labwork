#include<iostream>
#include<cmath>
#include<ctime>

using namespace std;

int main()
{
	srand(time(0));

	int month1,month2;
	int delta;
	month1 = rand() % 12 + 1;
	month2 = rand() % 12 + 1;
	delta = month1-month2;
	int deltadays = delta * 30;
	cout << "There are " << deltadays << " days between month " << month1 << " and " << month2 << endl;


	system("Pause");
	return 0;
}
