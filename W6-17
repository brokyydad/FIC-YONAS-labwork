#include<iostream>
#include<cmath>
using namespace std;

double vectorLength(float x1, float y1, float x2, float y2)
{
	double result = sqrt(x1 * x1 - 2 * x1 * x2 + x2 * x2 + y1 * y1 - 2 * y1 * y2 + y2 * y2);
	return result;
}

int main()
{
	int x1 = 1;
	int x2 = 3;
	int y1 = 1;
	int y2 = 3;

	double result = vectorLength(x1, y1, x2, y2);
	cout << result << endl;

	system("pause");
	return 0;

}
