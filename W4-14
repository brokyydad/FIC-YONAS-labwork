#include<iostream>

using namespace std;

int main()
{
	int n;
	cout << "enter n" << endl;
	cin >> n;
	while (n < 0)
	{
		cout << "mf enter the right n" << endl;
		cin >> n;
	}
	
	int a = 2;
	while (a < n)
	{
		if (n % a == 0)
		{
			cout << "The number is composite" << endl;
			break;
		}
		else
			a++;
	}
	if (a == n)
		cout << "The number is NOT composite" << endl;
	


	system("Pause");
	return 0;
}
