#include<iostream>
#include<cmath>
using namespace std;

int main()
{
	int n;
	cout << "enter n" << endl;
	cin >> n;
	while (n < 0)
	{
		cout << "mf enter the right n" << endl;
		cin >> n;
	}
	
	int a = 2;
	while (a < n)
	{
		if (n % a == 0)
		{
			cout << "This is not a prime number" << endl;
			break;
		}
		else
			a++;
	}
	if (a == n)
		cout << "this is a prime number" << endl;
	


	system("Pause");
	return 0;
}
